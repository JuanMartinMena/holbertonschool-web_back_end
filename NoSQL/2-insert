// Conéctate a la base de datos especificada en el comando mongo
var dbName = db.getSiblingDB("my_db");

// Inserta el documento en la colección `school`
var result = dbName.school.insertOne({
    name: "Holberton school"
});

// Mimic the WriteResult output format
print("WriteResult({ \"nInserted\" : " + (result.acknowledged ? 1 : 0) + " })");
